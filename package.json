{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "istanbul cover --dir='./static/' ./node_modules/mocha/bin/_mocha -- --reporter mochawesome --reporter-options reportDir='./static/' --timeout 100000 --recursive tests/"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "clui": "^0.3.1",
    "figlet": "^1.2.0",
    "mocha": "^3.4.1",
    "mochawesome": "^2.2.0",
    "mongoose": "^4.9.5",
    "redis": "^2.7.1",
    "request": "^2.81.0",
    "supertest": "^3.0.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "curly": true,
    "devel": true,
    "eqeqeq": true,
    "forin": true,
    "indent": 2,
    "jquery": true,
    "latedef": "nofunc",
    "maxerr": 50,
    "maxlen": 120,
    "maxparams": 4,
    "newcap": true,
    "noarg": true,
    "node": true,
    "nomen": true,
    "quotmark": "single",
    "plusplus": false,
    "strict": true,
    "undef": true,
    "unused": "strict"
  }
}
