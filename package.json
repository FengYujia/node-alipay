{
  "name": "alipay_api",
  "version": "2.1.3",
  "private": false,
  "scripts": {
    "start": "node index.js",
    "test": "istanbul cover --dir='./static/' ./node_modules/mocha/bin/_mocha -- --reporter mochawesome --reporter-options reportDir='./static/' --timeout 100000 --recursive tests/"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "mocha": "^3.4.1",
    "mochawesome": "^2.2.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "curly": true,
    "devel": true,
    "eqeqeq": true,
    "forin": true,
    "indent": 2,
    "jquery": true,
    "latedef": "nofunc",
    "maxerr": 50,
    "maxlen": 120,
    "maxparams": 4,
    "newcap": true,
    "noarg": true,
    "node": true,
    "nomen": true,
    "quotmark": "single",
    "plusplus": false,
    "strict": true,
    "undef": true,
    "unused": "strict"
  },
  "description": "## 新版支付宝API",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FengYujia/node-alipay.git"
  },
  "author": "FengYujia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FengYujia/node-alipay/issues"
  },
  "homepage": "https://github.com/FengYujia/node-alipay#readme"
}
